        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @list_of_listings.each do |a_listing| %>
      <tr>
        <td>
          <%= a_listing.id %>
        </td>

        <td>
          <%= a_listing.owner_id %>
        </td>

        <td>
          <%= a_listing.open_date %>
        </td>

        <td>
          <%= a_listing.close_date %>
        </td>

        <td>
          <%= a_listing.status %>
        </td>

        <td>
          <%= a_listing.address %>
        </td>

        <td>
          <%= a_listing.num_beds %>
        </td>

        <td>
          <%= a_listing.num_baths %>
        </td>

        <td>
          <%= a_listing.num_rooms %>
        </td>

        <td>
          <%= a_listing.num_beds_offered %>
