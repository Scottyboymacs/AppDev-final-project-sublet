<div>
  <div>
    <h1>
      List of all listings
    </h1>
  </div>
</div>

<hr>

<div>
  <div>
    <h2>
      Add a new listing
    </h2>

    <form action="/insert_listing" method="post">
      <div>
        <label for="owner_id_box">
          Owner
        </label>

        <input type="text" id="owner_id_box" name="query_owner_id">
      </div>

      <div>
        <label for="open_date_box">
          Open date
        </label>

        <input type="date" id="open_date_box" name="query_open_date">
      </div>

      <div>
        <label for="close_date_box">
          Close date
        </label>

        <input type="date" id="close_date_box" name="query_close_date">
      </div>

      <div>
        <input type="checkbox" id="status_box" name="query_status" value="1">

        <label for="status_box">Status</label>
      </div>

      <div>
        <label for="address_box">
          Address
        </label>

        <input type="text" id="address_box" name="query_address">
      </div>

      <div>
        <label for="num_beds_box">
          Num beds
        </label>

        <input type="text" id="num_beds_box" name="query_num_beds">
      </div>

      <div>
        <label for="num_baths_box">
          Num baths
        </label>

        <input type="text" id="num_baths_box" name="query_num_baths">
      </div>

      <div>
        <label for="num_rooms_box">
          Num rooms
        </label>

        <input type="text" id="num_rooms_box" name="query_num_rooms">
      </div>

      <div>
        <label for="num_beds_offered_box">
          Num beds offered
        </label>

        <input type="text" id="num_beds_offered_box" name="query_num_beds_offered">
      </div>

      <div>
        <label for="num_baths_box">
          Num baths
        </label>

        <input type="text" id="num_baths_box" name="query_num_baths">
      </div>

      <div>
        <label for="num_rooms_offered_box">
          Num rooms offered
        </label>

        <input type="text" id="num_rooms_offered_box" name="query_num_rooms_offered">
      </div>

      <div>
        <label for="region_id_box">
          Region
        </label>

        <input type="text" id="region_id_box" name="query_region_id">
      </div>

      <div>
        <label for="state_id_box">
          State
        </label>

        <input type="text" id="state_id_box" name="query_state_id">
      </div>

      <div>
        <label for="city_id_box">
          City
        </label>

        <input type="text" id="city_id_box" name="query_city_id">
      </div>

      <div>
        <label for="neighborhood_id_box">
          Neighborhood
        </label>

        <input type="text" id="neighborhood_id_box" name="query_neighborhood_id">
      </div>

      <div>
        <label for="building_id_box">
          Building
        </label>

        <input type="text" id="building_id_box" name="query_building_id">
      </div>

      <div>
        <input type="checkbox" id="furnished_box" name="query_furnished" value="1">

        <label for="furnished_box">Furnished</label>
      </div>

      <div>
        <label for="ammenities_box">
          Ammenities
        </label>

        <input type="text" id="ammenities_box" name="query_ammenities">
      </div>

      <div>
        <input type="checkbox" id="pets_box" name="query_pets" value="1">

        <label for="pets_box">Pets</label>
      </div>

      <div>
        <input type="checkbox" id="smoking_box" name="query_smoking" value="1">

        <label for="smoking_box">Smoking</label>
      </div>

      <div>
        <label for="sublease_category_box">
          Sublease category
        </label>

        <input type="text" id="sublease_category_box" name="query_sublease_category">
      </div>

      <div>
        <label for="gender_pref_box">
          Gender pref
        </label>

        <input type="text" id="gender_pref_box" name="query_gender_pref">
      </div>

      <div>
        <label for="floor_box">
          Floor
        </label>

        <input type="text" id="floor_box" name="query_floor">
      </div>

      <div>
        <label for="rent_box">
          Rent
        </label>

        <input type="text" id="rent_box" name="query_rent">
      </div>

      <div>
        <label for="dates_flex_box">
          Dates flex
        </label>

        <input type="text" id="dates_flex_box" name="query_dates_flex">
      </div>

      <div>
        <label for="start_flex_left_box">
          Start flex left
        </label>

        <input type="text" id="start_flex_left_box" name="query_start_flex_left">
      </div>

      <div>
        <label for="start_flex_right_box">
          Start flex right
        </label>

        <input type="text" id="start_flex_right_box" name="query_start_flex_right">
      </div>

      <div>
        <label for="end_flex_left_box">
          End flex left
        </label>

        <input type="text" id="end_flex_left_box" name="query_end_flex_left">
      </div>

      <div>
        <label for="end_flex_right_box">
          End flex right
        </label>

        <input type="text" id="end_flex_right_box" name="query_end_flex_right">
      </div>

      <div>
        <label for="notes_box">
          Notes
        </label>

        <input type="text" id="notes_box" name="query_notes">
      </div>

      <div>
        <label for="map_url_box">
          Map url
        </label>

        <input type="text" id="map_url_box" name="query_map_url">
      </div>

      <div>
        <label for="website_url_box">
          Website url
        </label>

        <input type="text" id="website_url_box" name="query_website_url">
      </div>

      <div>
        <label for="satellite_url_box">
          Satellite url
        </label>

        <input type="text" id="satellite_url_box" name="query_satellite_url">
      </div>

      <div>
        <label for="num_watching_box">
          Num watching
        </label>

        <input type="text" id="num_watching_box" name="query_num_watching">
      </div>

      <div>
        <label for="visibility_box">
          Visibility
        </label>

        <input type="text" id="visibility_box" name="query_visibility">
      </div>

      <button>
        Create listing
      </button>
    </form>
  </div>
</div>

<hr>
<div>
  <div>
    <table border="1">
      <tr>
        <th>
          ID
        </th>

        <th>
          Owner
        </th>

        <th>
          Open date
        </th>

        <th>
          Close date
        </th>

        <th>
          Status
        </th>

        <th>
          Address
        </th>

        <th>
