        <td>
          <%= a_listing.notes %>
        </td>

        <td>
          <%= a_listing.map_url %>
        </td>

        <td>
          <%= a_listing.website_url %>
        </td>

        <td>
          <%= a_listing.satellite_url %>
        </td>

        <td>
          <%= a_listing.num_watching %>
        </td>

        <td>
          <%= a_listing.visibility %>
        </td>

        <td>
          <%= time_ago_in_words(a_listing.created_at) %> ago
        </td>
        <td>
          <%= time_ago_in_words(a_listing.updated_at) %> ago
        </td>

        <td>
          <a href="/listings/<%= a_listing.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
